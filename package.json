{
  "name": "action-descriptor-navigator",
  "version": "4.1.0",
  "description": "LINQ-style navigation framework for Adobe Photoshop ActionDescriptor with ES3 compatibility and comprehensive testing",
  "main": "ActionDescriptorNavigator.ts",
  "types": "types.ts",
  "scripts": {
    "build": "tsc",
    "build:webpack": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "transpile": "tsc",
    "transpile:es3": "webpack --config webpack.config.js",
    "generate-test-psds": "ts-node tests/generate-test-assets.ts",
    "generate-xml-scripts": "ts-node tests/generate-xml-dumps.ts",
    "create-expected-results": "ts-node tests/create-expected-results.ts",
    "test:integration": "npm run generate-test-psds && echo 'PSDs generated. Run on work computer: Open PSDs in Photoshop, then run XML scripts.'",
    "validate-setup": "ts-node --version && echo 'Setup validated successfully'"
  },
  "keywords": [
    "photoshop",
    "extendscript",
    "actiondescriptor",
    "navigation",
    "linq",
    "scoring",
    "es3",
    "testing",
    "automation"
  ],
  "author": "Framework Integration",
  "license": "MIT",
  "devDependencies": {
    "@types/lodash": "^4.17.7",
    "@types/node": "^18.19.0",
    "types-for-adobe": "^7.2.2",
    "typescript": "^4.1.6",
    "webpack": "^4.17.0",
    "webpack-cli": "^3.3.0",
    "webpack-es3-plugin": "^1.5.2",
    "webpack-merge": "^4.1.3"
  },
  "engines": {
    "extendscript": ">=3.0",
    "node": ">=14.0.0"
  },
  "files": [
    "ActionDescriptorNavigator.ts",
    "types.ts",
    "es5-polyfills.js",
    "tests/assets/*.psd",
    "tests/expected/*.json",
    "tests/scripts/*.jsx"
  ],
  "dependencies": {
    "@types/jest": "^29.5.0",
    "@types/xml2js": "^0.4.14",
    "ag-psd": "^28.2.2",
    "jest": "^29.5.0",
    "lodash": "^4.17.21",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "xml2js": "^0.6.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["**/tests/**/*.test.ts"],
    "collectCoverageFrom": [
      "ActionDescriptorNavigator.ts",
      "types.ts"
    ]
  }
}