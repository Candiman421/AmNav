{
  "name": "actionmanager-navigator",
  "version": "1.0.0",
  "description": "Fluent ActionManager Navigator for Adobe ExtendScript - Criteria-based selection with sentinel error handling",
  "main": "dist/ActionDescriptorNavigator.js",
  "types": "dist/types.d.ts",
  "scripts": {
    "webpack": "webpack",
    "dev": "webpack --mode=development",
    "prod": "webpack --mode=production",
    "build": "npm run prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "generate-assets": "npx ts-node tests/generate-test-assets.ts",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prebuild": "npm run clean",
    "postbuild": "echo 'âœ… Build complete: dist/ActionDescriptorNavigator.js ready for ExtendScript'",
    "pretest": "npm run lint",
    "validate": "npm run lint && npm run test && npm run build",
    "dev-quick": "npm run dev && echo 'ðŸš€ Development build ready'",
    "prod-ready": "npm run validate && npm run prod && echo 'ðŸŽ¯ Production build validated and ready'",
    "parse-xml": "npx ts-node tests/parse-xml-expectations.ts",
    "build-tests": "npm run build && npm run parse-xml && echo 'âœ… Tests ready for Photoshop'",
    "copy-tests": "npm run build-tests && echo 'Copy tests/extendscript-tests/*.js to main computer'"
  },
  "keywords": [
    "photoshop",
    "extendscript",
    "actionmanager",
    "adobe",
    "after-effects",
    "illustrator",
    "indesign",
    "premiere-pro",
    "fluent-api",
    "typescript",
    "es3",
    "actiondescriptor",
    "text-analysis",
    "font-extraction"
  ],
  "author": "ActionManager Navigator Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CandiMan421/AmNav.git"
  },
  "bugs": {
    "url": "https://github.com/CandiMan421/AmNav/issues"
  },
  "homepage": "https://github.com/CandiMan421/AmNav#readme",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^22.13.13",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "ag-psd": "^14.3.6",
    "eslint": "^7.32.0",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.5",
    "ts-loader": "^8.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.1.6",
    "webpack": "^4.47.0",
    "webpack-cli": "^3.3.0",
    "webpack-es3-plugin": "^1.5.2",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "npx": "^10.2.2"
  },
  "files": [
    "dist/**/*",
    "ActionManager/types.ts",
    "es5-polyfills.js",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ]
}